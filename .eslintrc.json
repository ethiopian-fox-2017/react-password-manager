{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
           "extensions": [".js", ".jsx"]
        }
      ]
    },
    "globals": {
      "test": true,
      "it": true,
      "describe": true
    }
}
